{
  "name": "task-prioritization-eisenhower-matrix",
  "version": "0.0.1",
  "description": "Initial version to setup the repositories",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "start-dev": "electron .",
    "lint": "biome ci",
    "fix": "biome check --write",
    "test": "jest --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Team-07-CSE-210-FA-24/cse210-fa24-group07.git"
  },
  "author": "Sohan",
  "license": "ISC",
  "build": {
    "appId": "com.taskprioritization.eisenhowermatrix",
    "productName": "Task Prioritization Eisenhower Matrix",
    "linux": {
      "category": "Utility",
      "icon": "build/icons",
      "target": [
        "AppImage",
        "snap"
      ]
    },
    "files": [
      "dist/**/*",
      "renderer/**/*",
      "main.js",
      "preload.js"
    ],
    "directories": {
      "output": "dist"
    },
    "publish": {
      "provider": "github",
      "owner": "Team-07-CSE-210-FA-24",
      "repo": "cse210-fa24-group07"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@biomejs/biome": "^1.9.4",
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-rpm": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
    "@electron-forge/plugin-fuses": "^7.5.0",
    "@electron/fuses": "^1.8.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "babel-jest": "^29.7.0",
    "cssom": "^0.5.0",
    "electron": "^33.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc": "^4.0.4",
    "jsdom": "^25.0.1",
    "jsesc": "^3.0.2"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.0"
  }
}
