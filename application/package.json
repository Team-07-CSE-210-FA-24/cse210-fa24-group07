{
  "name": "task-prioritization-eisenhower-matrix",
  "version": "0.0.1",
  "description": "Initial version to setup the repositories",
  "main": "main.js",
  "scripts": {
    "build": "electron-builder",
    "start": "electron ."
  },
  "build": {
    "appId": "com.UCSD.team7",
    "productName": "EisenhowerMatrix",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "preload.js",
      "renderer/view.html",
      "renderer/index.html",
      "renderer/renderer.js",
      "node_modules/**/*"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "author": "Sohan",
  "license": "ISC",
  "devDependencies": {
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8"
  }
}
